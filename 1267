a=int(input())
b=input()
b=b.split(" ")
for i in range(len(b)):
    b[i]=int(b[i])
result_y=0
for i in range(len(b)):
    result_y+=((b[i]//30)+1) *10    
result_m=0
for i in range(len(b)):
    result_m+=((b[i]//60)+1)*15
if result_y> result_m:
    print('M',result_m)
elif result_y==result_m:
    print('Y M',result_m)
else:
    print('Y', result_y)
    
    
